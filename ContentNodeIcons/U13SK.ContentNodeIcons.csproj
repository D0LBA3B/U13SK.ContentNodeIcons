<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
		<Version>1.0.0</Version>
		<Authors>Mark Drake</Authors>
		<Company>Mark Drake</Company>
		<Description>This allows for custom icons to be assigned to individual content nodes in the Umbraco CMS. 
    Part of the Umbraco 13 Starter Kit (U13SK).</Description>
		<Copyright>Copyright Â© 2020 Mark Drake, and other contributors</Copyright>
		<PackageProjectUrl>https://github.com/markadrake/U8SK.ContentNodeIcons</PackageProjectUrl>
		<RepositoryUrl>https://github.com/markadrake/U8SK.ContentNodeIcons</RepositoryUrl>
		<RepositoryType>Git</RepositoryType>
		<PackageTags>Umbraco</PackageTags>
		<PackageReleaseNotes>This is an beta release. Please report any issues to the public repository.</PackageReleaseNotes>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<NeutralLanguage>en</NeutralLanguage>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
	</PropertyGroup>

	<ItemGroup>
	  <None Remove="App_Plugins\U13SK.ContentNodeIcons\package.manifest" />
	  <None Remove="App_Plugins\U13SK.ContentNodeIcons\tree.action.setIconController.js" />
	  <None Remove="App_Plugins\U13SK.ContentNodeIcons\tree.action.setIconTemplate.html" />
	</ItemGroup>

	<ItemGroup>
	  <Content Include="App_Plugins\U13SK.ContentNodeIcons\package.manifest" />
	  <Content Include="App_Plugins\U13SK.ContentNodeIcons\tree.action.setIconController.js" />
	  <Content Include="App_Plugins\U13SK.ContentNodeIcons\tree.action.setIconTemplate.html" />
	</ItemGroup>

	<ItemGroup>
    <PackageReference Include="Umbraco.Cms.Web.Website" Version="13.0.0" />
    <PackageReference Include="Umbraco.Cms.Web.BackOffice" Version="13.0.0" />
	</ItemGroup>
</Project>
